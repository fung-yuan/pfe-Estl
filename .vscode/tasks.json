{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "start-vite",
			"type": "shell",
			"command": "cd ${workspaceFolder}/client && npm run dev",
			"isBackground": true,
			"problemMatcher": {
				"pattern": {
					"regexp": "^.*$"
				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".*",
					"endsPattern": "Local:.*"
				}
			}
		}
	]
}